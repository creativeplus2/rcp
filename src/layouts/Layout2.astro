---
import Layout from "../layouts/Layout.astro";
import Banner2 from "../components/Banner2.astro";
import Footer from "../components/Footer.astro";
const { frontmatter } = Astro.props;
---

<Layout title={"PT RAHAYU CARITA PERKASA |" + frontmatter.titlebanner}>
	<Banner2
		title={frontmatter.titlebanner}
		image={frontmatter.imagerbanner}
	/>

	<div class="row-small py-[2em] lg:py-[6.5em] prose">
		<slot />
	</div>

	<Footer />
</Layout>
